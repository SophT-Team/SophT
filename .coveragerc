[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    pass
    def __repr__
    from
    import
    # pystencil kernel symbolic stuff
    ps.fields
    sp.symbols
    @=
show_missing = true

[run]
branch = True

omit =
    # omit anything in a .local directory anywhere
    */.local/*
    */experimental/*
    */rod_viz.py
